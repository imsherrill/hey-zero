generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator zero {
  provider       = "prisma-generator-zero"
  output         = "../zero"
  schemaVersion  = "1"

}

model User {
  id    Int    @id @default(autoincrement())
  name  String @db.VarChar(255)

  @@map("users")
}
